{
    "name": "my-app",
    "version": "1.0.0",
    "main": "index.ts",
    "type": "module",
    "scripts": {
        "backend:test": "echo \"Error: no test specified\" && exit 1",
        "frontend:start": "expo start",
        "frontend:reset-project": "node ./scripts/reset-project.js",
        "frontend:android": "expo start --android",
        "frontend:ios": "expo start --ios",
        "frontend:web": "expo start --web",
        "frontend:test": "jest --watchAll",
        "frontend:lint": "expo lint",
        "start": "concurrently \"npm run frontend:start\" \"ts-node -r tsconfig-paths/register backend/index.ts\""
    },
    "dependencies": {
        "@expo/vector-icons": "^14.0.2",
        "@prisma/client": "^5.22.0",
        "@react-navigation/bottom-tabs": "6.5.20",
        "@react-navigation/native": "6.1.18",
        "@react-navigation/stack": "6.3.14",
        "axios": "^1.7.7",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "expo": "~51.0.28",
        "expo-constants": "~16.0.2",
        "expo-font": "~12.0.9",
        "expo-linking": "~6.3.1",
        "expo-router": "^3.5.23",
        "expo-splash-screen": "~0.27.5",
        "expo-status-bar": "~1.12.1",
        "expo-system-ui": "~3.0.7",
        "expo-web-browser": "~13.0.3",
        "express": "^4.21.1",
        "metro": "^0.81.0",
        "prisma": "^5.22.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-native": "^0.74.5",
        "react-native-gesture-handler": "~2.16.1",
        "react-native-reanimated": "~3.10.1",
        "react-native-safe-area-context": "^4.14.0",
        "react-native-screens": "^4.0.0",
        "react-native-web": "~0.19.10",
        "sqlite3": "^5.1.7"
    },
    "devDependencies": {
        "@babel/core": "^7.20.0",
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/jest": "^29.5.12",
        "@types/node": "^22.9.0",
        "@types/react": "~18.2.45",
        "@types/react-native": "^0.73.0",
        "@types/react-test-renderer": "^18.0.7",
        "concurrently": "^9.1.0",
        "jest": "^29.2.1",
        "jest-expo": "~51.0.3",
        "react-test-renderer": "18.2.0",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.6.3"
    },
    "jest": {
        "preset": "jest-expo"
    },
    "private": true,
    "ts-node": {
        "compilerOptions": {
            "module": "ESNext",
            "moduleResolution": "node",
            "esModuleInterop": true,
            "allowJs": true,
            "skipLibCheck": true,
            "strict": true
        },
        "require": [
            "tsconfig-paths/register"
        ]
    }
}
